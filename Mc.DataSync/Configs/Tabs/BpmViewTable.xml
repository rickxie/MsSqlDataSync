<?xml version='1.0' encoding='utf-8'?>
<ReleaseBuildConfig>
  <!-- Tab展示配置 -->
  <TabConfig>
    <!-- Tab排序号 -->
    <No>2</No>
    <!-- Tab显示名称 -->
    <Name>视图</Name>
    <!-- 对应生成脚本sql -->
    <Handler>workflow.sql</Handler>
    <!-- 对应显示数据脚本,必须要将作为参数的列查询出来,必须要有Id和text列 -->
    <GetSql>SELECT SchemaName AS [text] ,Id as ViewTableId,* FROM BpmViewTable</GetSql>
    <!-- 传递参数配置 -->
    <ReplaceParams>
      <ReplaceParam>
        <!-- 参数列字段  -->
        <ParamName>Id</ParamName>
        <!-- 参数替换变量名称 -->
        <InputColunmnName>ViewTableId</InputColunmnName>
      </ReplaceParam>
    </ReplaceParams>
  </TabConfig>
  <!-- 对比配置 -->
  <ComparisonConfig>
    <ComparisonTables>

      <!-- 对比表配置项 BpmViewTable -->
      <ComparisonTable>
        <!-- 表名 -->
        <TableName>BpmViewTable</TableName>
        <!-- 是否为校验主表,方便外层对比是否为新增,加快速度 -->
        <IsMasterTable>true</IsMasterTable>
        <!-- 主键 -->
        <PrimaryKey>Id</PrimaryKey>
        <!-- 需要对比的列 -->
        <ComparisonColumns>
          <ComparisonColumn>
            <!-- 查询列字段或者函数 -->
            <ComparisonSelectColumn>Id</ComparisonSelectColumn>
            <!-- 显示字段名称,如果为空则默认使用 ComparisonSelectColumn中的值 -->
            <ComparisonShowColumn></ComparisonShowColumn>
          </ComparisonColumn>
          <ComparisonColumn>
            <!-- 查询列字段或者函数 -->
            <ComparisonSelectColumn>SchemaName</ComparisonSelectColumn>
            <!-- 显示字段名称,如果为空则默认使用 ComparisonSelectColumn中的值 -->
            <ComparisonShowColumn></ComparisonShowColumn>
          </ComparisonColumn>
          <ComparisonColumn>
            <!-- 查询列字段或者函数 -->
            <ComparisonSelectColumn>DataSql</ComparisonSelectColumn>
            <!-- 显示字段名称,如果为空则默认使用 ComparisonSelectColumn中的值 -->
            <ComparisonShowColumn></ComparisonShowColumn>
          </ComparisonColumn>

          <ComparisonColumn>
            <!-- 查询列字段或者函数 -->
            <ComparisonSelectColumn>LangName</ComparisonSelectColumn>
            <!-- 显示字段名称,如果为空则默认使用 ComparisonSelectColumn中的值 -->
            <ComparisonShowColumn></ComparisonShowColumn>
          </ComparisonColumn>
          <ComparisonColumn>
            <!-- 查询列字段或者函数 -->
            <ComparisonSelectColumn>LangDescription</ComparisonSelectColumn>
            <!-- 显示字段名称,如果为空则默认使用 ComparisonSelectColumn中的值 -->
            <ComparisonShowColumn></ComparisonShowColumn>
          </ComparisonColumn>
          <ComparisonColumn>
            <!-- 查询列字段或者函数 -->
            <ComparisonSelectColumn>SearchSql</ComparisonSelectColumn>
            <!-- 显示字段名称,如果为空则默认使用 ComparisonSelectColumn中的值 -->
            <ComparisonShowColumn></ComparisonShowColumn>
          </ComparisonColumn>
          <ComparisonColumn>
            <!-- 查询列字段或者函数 -->
            <ComparisonSelectColumn>Type</ComparisonSelectColumn>
            <!-- 显示字段名称,如果为空则默认使用 ComparisonSelectColumn中的值 -->
            <ComparisonShowColumn></ComparisonShowColumn>
          </ComparisonColumn>
  
        </ComparisonColumns>
        <!-- 对比数据筛选条件 -->
        <ComparisonCondition>
          <!-- 筛选数据语句 -->
          <ConditionSql>
            Id = @ViewTableId
          </ConditionSql>
          <!-- 传递参数配置 -->
          <ReplaceParams>
            <ReplaceParam>
              <!-- 执行sql参数化字段名称  -->
              <ParamName>ViewTableId</ParamName>
              <!-- 传入参数化获取数据时传入的字段名称 -->
              <InputColunmnName>ViewTableId</InputColunmnName>
            </ReplaceParam>
          </ReplaceParams>
        </ComparisonCondition>
      </ComparisonTable>

