        </ComparisonColumns>
        <!-- 对比数据筛选条件 -->
        <ComparisonCondition>
          <!-- 筛选数据语句 -->
          <ConditionSql>
            ViewTableId = @ViewTableId
          </ConditionSql>
          <!-- 传递参数配置 -->
          <ReplaceParams>
            <ReplaceParam>
              <!-- 执行sql参数化字段名称  -->
              <ParamName>ViewTableId</ParamName>
              <!-- 传入参数化获取数据时传入的字段名称 -->
              <InputColunmnName>ViewTableId</InputColunmnName>
